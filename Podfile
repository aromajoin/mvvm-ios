# Uncomment the next line to define a global platform for your project
platform :ios, '13.0'

target 'mvvm-ios' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for mvvm-ios
  pod 'RxSwift'
  pod 'RxCocoa'
  pod 'RealmSwift'
end

target 'mvvm-ios-tests' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!
  
  # Pods for mvvm-ios
  pod 'RxSwift'
  pod 'RxCocoa'
  pod 'RealmSwift'
end
